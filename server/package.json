{
  "name": "server",
  "version": "0.1.1",
  "description": "SUP server for web and API",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "tsc --pretty",
    "coverage": "jest --coverage",
    "start": "nodemon src/index.ts",
    "db:fixtures:load": "ts-node src/database/fixtures/load.ts",
    "db:migration:create": "ts-node ./node_modules/.bin/typeorm migration:create",
    "db:migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "db:migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "db:migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cverna-hackathons/sup.git"
  },
  "keywords": [
    "sup",
    "server"
  ],
  "author": "Peter Berezny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cverna-hackathons/sup/issues"
  },
  "homepage": "https://github.com/cverna-hackathons/sup#readme",
  "dependencies": {
    "@decorators/di": "^1.0.2",
    "@decorators/express": "^2.3.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/multer": "^1.3.10",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^12.7.11",
    "@types/pg": "^7.11.2",
    "@types/uniqid": "^4.1.3",
    "aws-sdk": "^2.814.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.10.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "imagemagick": "^0.1.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.19",
    "typescript": "^3.6.3",
    "uniqid": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/imagemagick": "0.0.30",
    "@types/morgan": "^1.7.37",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typeorm-fixtures-cli": "^1.3.7"
  }
}
